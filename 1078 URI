#include <stdio.h>
 
int main() {
 
    int x,y;
    scanf("%d",&x);
    y=1*x;
    printf("1 x %d = %d\n",x,y);
    y=2*x;
    printf("2 x %d = %d\n",x,y);
    y=3*x;
    printf("3 x %d = %d\n",x,y);
    y=4*x;
    printf("4 x %d = %d\n",x,y);
    y=5*x;
    printf("5 x %d = %d\n",x,y);
    y=6*x;
    printf("6 x %d = %d\n",x,y);
    y=7*x;
    printf("7 x %d = %d\n",x,y);
    y=8*x;
    printf("8 x %d = %d\n",x,y);
    y=9*x;
    printf("9 x %d = %d\n",x,y);
    y=10*x;
    printf("10 x %d = %d\n",x,y);
 
    return 0;
}
